    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.13",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^11.0.13",
    "@nestjs/swagger": "^7.0.0",
