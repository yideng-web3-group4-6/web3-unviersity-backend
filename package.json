    "@nestjs/cli": "^8.0.0",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nx/esbuild": "^20.7.1",
    "@nx/eslint": "^20.7.1",
    "@nx/webpack": "^20.7.1",
    "@types/aws-lambda": "^8.10.148",
    "@types/express": "^4.17.21",
    "@types/jest": "27.5.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^4.0.1",
